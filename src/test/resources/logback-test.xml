<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <jmxConfigurator />
  <appender name="stdout" class="ch.qos.logback.core.ConsoleAppender">
    <layout class="ch.qos.logback.classic.PatternLayout">
      <pattern>%d %r %p %t %c{1} %m%n</pattern>
    </layout>
  </appender>
  <root>
    <level value="DEBUG"/>
    <appender-ref ref="stdout"/>
  </root>
</configuration>
